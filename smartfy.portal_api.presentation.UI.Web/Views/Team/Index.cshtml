<fieldset class="fieldset">
    <legend>Listagem de Equipe</legend>
    <div class="row-fluid">
        <div class="span6">
            @Html.AntiForgeryToken()
            <table id="dt" class="dt table table-striped table-bordered"
                   style="width:100%"
                   data-url="/Team/List"
                   data-cols="code,plate,area,partner"
                   data-what="Listagem de Equipe"
                   data-delete-url="/Team/Delete"
                   data-create-url="/Team/Create"
                   data-edit-url="/Team/Edit">
                <thead>
                    <tr>
                        <th>Número Serial</th>
                        <th>Placa</th>
                        <th>Área</th>
                        <th>PArceiro</th>
                        <th data-orderable="false" class="w-110"></th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</fieldset>
<br />